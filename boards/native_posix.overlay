/ {
    gpio_input: gpio_input {
        compatible = "gpio-emul";
        gpios = <&gpio0 0 GPIO_PULL_UP>;
        label = "GPIO_INPUT";
    };

    gpio_output: gpio_output {
        compatible = "gpio-emul";
        gpios = <&gpio0 1 GPIO_ACTIVE_HIGH>;
        label = "GPIO_OUTPUT";
    };

};